{
  "extends": "airbnb/base",

  "parser": "babel-eslint",

  "rules": {
    "id-length": 0,
    "func-names": 0,
    "comma-dangle": [2, "never"],
    "no-param-reassign": 0,
    "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }]
  },

  "globals": {
    "describe": false,
    "xdescribe": false,
    "it": false,
    "beforeEach": false,
    "afterEach": false,
    "expect": false,
    "spyOn": false,
    "jasmine": false
  }
}
